
show databases;

Create database sb006;


USE sb006;

CREATE TABLE STUDENT(
     	STU_ID int NOT NULL PRIMARY KEY,
     	STU_AGE int NOT NULL,
    	STU_FNAME varchar (255),
     	STU_LAST varchar (255)
     );
